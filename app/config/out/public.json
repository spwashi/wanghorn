{
   "name": "wanghorn",
   "appDomain": "http://localhost",
   "appUrl": "http://localhost/wanghorn",
   "appPath": "wanghorn",
   "appPublicUrl": "http://localhost/wanghorn/public",
   "routes": {
      "error-404": {
         "pattern": "wanghorn/404/{path}"
      },
      "home": {
         "pattern": "wanghorn/$",
         "title": "Home"
      },
      "about_me": {
         "pattern": "wanghorn/about-me$",
         "title": "About Me"
      },
      "gallery--home": {
         "pattern": "wanghorn/gallery$",
         "title": "Gallery"
      },
      "gallery--item__view": {
         "pattern": "wanghorn/gallery/{name}:[a-zA-Z]+",
         "title": "Gallery Item View"
      },
      "gallery--items": {
         "pattern": "wanghorn/gallery/items.json$"
      },
      "dev--home": {
         "pattern": "wanghorn/dev$",
         "title": "Dev"
      },
      "dev--monitors": {
         "pattern": "wanghorn/dev/monitors.json$"
      },
      "dev--models": {
         "pattern": "wanghorn/dev/models.json$"
      },
      "dev--entities": {
         "pattern": "wanghorn/dev/entities.json$"
      },
      "dev--routes": {
         "pattern": "wanghorn/dev/routes.json$"
      },
      "dev--execute_query": {
         "pattern": "wanghorn/dev/models/{smID}:[\\[a-zA-Z\\]\\s]+/execute/{query}:[a-zA-Z_]+"
      },
      "dev--model": {
         "pattern": "wanghorn/dev/models/{smID}:[\\[a-zA-Z\\]\\s]+$"
      },
      "dev--get_model_schematic": {
         "pattern": "wanghorn/dev/models/{smID}:[\\[a-zA-Z\\]\\s]+/schematic$",
         "http_method": [
            "get"
         ]
      },
      "dev--all_models": {
         "pattern": "wanghorn/dev/models/{smID}:[\\[a-zA-Z\\]\\s]+/all",
         "http_method": [
            "get"
         ]
      },
      "dev--model--home": {
         "pattern": "wanghorn/dev/models$"
      },
      "dev--create_model": {
         "pattern": "wanghorn/dev/models/{smID}:[\\[a-zA-Z\\]\\s]+/create$"
      },
      "dev--create_model--receive": {
         "pattern": "wanghorn/dev/models/{smID}:[\\[a-zA-Z\\]\\s]+/create/receive$",
         "http_method": [
            "post"
         ]
      },
      "user--signup_continue": {
         "pattern": "wanghorn/dev/email/test/{signup_nonce}"
      },
      "user--login": {
         "pattern": "wanghorn/user/login$"
      },
      "user--logout$": {
         "pattern": "wanghorn/user/logout$"
      },
      "user--profile__json": {
         "pattern": "wanghorn/user/{id}:[a-zA-Z@.]+$"
      },
      "user--process_signup": {
         "pattern": "wanghorn/user/signup/receive"
      }
   }
}