{
   "name": "wanghorn",
   "appDomain": "http://localhost",
   "appUrl": "http://localhost/wanghorn",
   "appPath": "wanghorn",
   "appPublicUrl": "http://localhost/wanghorn/public",
   "routes": {
      "error-404": {
         "pattern": "wanghorn/404/{path}"
      },
      "home": {
         "pattern": "wanghorn/$"
      },
      "about_me": {
         "pattern": "wanghorn/about-me$"
      },
      "gallery--home": {
         "pattern": "wanghorn/gallery$"
      },
      "gallery--items": {
         "pattern": "wanghorn/gallery/items.json$"
      },
      "dev--home": {
         "pattern": "wanghorn/dev$"
      },
      "dev--monitors": {
         "pattern": "wanghorn/dev/monitors.json$"
      },
      "dev--models": {
         "pattern": "wanghorn/dev/models.json$"
      },
      "dev--routes": {
         "pattern": "wanghorn/dev/routes.json$"
      },
      "dev--execute_query": {
         "pattern": "wanghorn/dev/model/{smID}:[\\[a-zA-Z\\]\\s]+/execute/{query}:[a-zA-Z_]+"
      },
      "dev--get_model_schematic": {
         "pattern": "wanghorn/dev/model/{smID}:[\\[a-zA-Z\\]\\s]+$",
         "http_method": [
            "get"
         ]
      },
      "dev--create_model": {
         "pattern": "wanghorn/dev/model/{smID}:[\\[a-zA-Z\\]\\s]+/create",
         "http_method": [
            "post"
         ]
      },
      "user--signup_continue": {
         "pattern": "wanghorn/dev/email/test/{signup_nonce}"
      },
      "user--login": {
         "pattern": "wanghorn/user/login$"
      },
      "user--profile__json": {
         "pattern": "wanghorn/user/{id}:[a-zA-Z@.]+$"
      },
      "user--process_signup": {
         "pattern": "wanghorn/user/signup/receive"
      }
   }
}