{
   "name": "wanghorn",
   "appDomain": "http://localhost",
   "appUrl": "http://localhost/wanghorn",
   "appPath": "wanghorn",
   "appPublicUrl": "http://localhost/wanghorn/public",
   "routes": {
      "error-404": {
         "pattern": "wanghorn/404/{path}"
      },
      "home": {
         "pattern": "wanghorn/$",
         "title": "Home"
      },
      "about_me": {
         "pattern": "wanghorn/about-me$",
         "title": "About Me"
      },
      "gallery--home": {
         "pattern": "wanghorn/gallery$",
         "title": "Gallery"
      },
      "gallery--item__view": {
         "pattern": "wanghorn/gallery/{name}:[a-zA-Z]+",
         "title": "Gallery Item View"
      },
      "gallery--items": {
         "pattern": "wanghorn/gallery/items.json$"
      },
      "events--home": {
         "pattern": "wanghorn/events$",
         "title": "Events"
      },
      "events--item__view": {
         "pattern": "wanghorn/events/{id}:.+/view",
         "title": "Event View"
      },
      "dev--home": {
         "pattern": "wanghorn/dev$",
         "title": "Dev"
      },
      "dev--model": {
         "pattern": "wanghorn/dev/models/{name}:[a-zA-Z_]+$"
      },
      "dev--models": {
         "pattern": "wanghorn/dev/models$",
         "title": "Models"
      },
      "dev--models__json": {
         "pattern": "wanghorn/dev/models.json$"
      },
      "dev--model--property": {
         "pattern": "wanghorn/dev/models/{owner}:[a-zA-Z_]+/property/{property}:[a-zA-Z_]+$",
         "title": "Model Property"
      },
      "dev--all_models": {
         "pattern": "wanghorn/dev/models/{name}:[a-zA-Z_]+/all",
         "http_method": [
            "get"
         ]
      },
      "dev--get_model_schematic": {
         "pattern": "wanghorn/dev/models/{name}:[a-zA-Z_]+/schematic$",
         "http_method": [
            "get"
         ]
      },
      "dev--model--home": {
         "pattern": "wanghorn/dev/models$"
      },
      "dev--create_model": {
         "pattern": "wanghorn/dev/models/{name}:[a-zA-Z_]+/create$"
      },
      "dev--create_model--receive": {
         "pattern": "wanghorn/dev/models/{name}:[a-zA-Z_]+/create/receive$",
         "http_method": [
            "post"
         ]
      },
      "dev--entity--property": {
         "pattern": "wanghorn/dev/entities/{owner}:[a-zA-Z_]+/property/{property}:[a-zA-Z_]+$",
         "title": "Entity Property"
      },
      "dev--entities__json": {
         "pattern": "wanghorn/dev/entities.json$"
      },
      "dev--monitors__json": {
         "pattern": "wanghorn/dev/monitors.json$"
      },
      "dev--status__json": {
         "pattern": "wanghorn/dev/status.json$"
      },
      "dev--routes__json": {
         "pattern": "wanghorn/dev/routes.json$"
      },
      "dev--execute_query": {
         "pattern": "wanghorn/dev/models/{smID}:[\\[_a-zA-Z\\]\\s]+/execute/{query}:[a-zA-Z_]+"
      },
      "user--signup_continue": {
         "pattern": "wanghorn/dev/email/test/{signup_nonce}"
      },
      "user--process_login": {
         "pattern": "wanghorn/user/login$"
      },
      "user--logout$": {
         "pattern": "wanghorn/user/logout$"
      },
      "user--profile__json": {
         "pattern": "wanghorn/user/{id}:[a-zA-Z@.]+$"
      },
      "user--process_signup": {
         "pattern": "wanghorn/user/signup/receive"
      }
   }
}