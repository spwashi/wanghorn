{
    "pattern_prefix": "wanghorn/",

    "routes":         [
        {
            "name":       "404",

            "pattern":    "404/{path}",
            "resolution": "Error@rt_404"
        },

        {
            "name":       "home",

            "pattern":    "$",
            "resolution": "#Home::index"
        },
        {
            "name":       "loaded_routes",

            "pattern":    "dev/monitors",
            "resolution": "#Dev\\Dev::monitors"
        },

        {
            "pattern":    "test",
            "resolution": "#Home::item"
        },

        {
            "pattern":    "user/login$",
            "resolution": "User\\User@login"
        },
        {
            "pattern":    "user/{id}:\\d+",
            "resolution": "User\\User@userByID"
        },

        {
            "pattern":    "one",
            "resolution": "#Temp::react_1"
        },
        {
            "pattern":    "one/{id}:[a-zA-Z]*",
            "resolution": "#Temp::react_1"
        },

        {
            "pattern":    "example",
            "resolution": "#Dev\\Dev::eg"
        },
        {
            "pattern":    "dev/models",
            "resolution": "#Dev\\Dev::modelsToTables"
        }
    ]
}